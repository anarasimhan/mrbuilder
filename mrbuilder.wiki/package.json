{
  "name": "mrbuilder.wiki",
  "private": true,
  "version": "0.9.3",
  "dependencies": {
    "mrbuilder": "^0.9.3",
    "mrbuilder-plugin-react-styleguidist": "^0.9.3"
  },
  "scripts": {
    "prepublish": "mrbuilder-webpack --app ../../mr-builder.github.io",
    "start": "mrbuilder-webpack-dev-server"
  },
  "mrbuilder": {
    "plugin": "src/index",
    "plugins": [
      [
        "mrbuilder-plugin-html",
        false
      ],
      [
        "mrbuilder-plugin-hot",
        false
      ],
      "mrbuilder-plugin-doc-prop-types",
      "mrbuilder",
      [
        "mrbuilder-plugin-webpack",
        {
          "app": "docs"
        }
      ],
      "mrbuilder.wiki"
    ]
  }
}
