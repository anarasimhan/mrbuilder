{
  "name": "mrbuilder.wiki",
  "private": true,
  "version": "2.0.0-beta.1",
  "dependencies": {
    "mrbuilder": "^2.0.0-beta.1",
    "mrbuilder-plugin-clean": "^2.0.0-beta.1",
    "mrbuilder-plugin-react-styleguidist": "^2.0.0-beta.1"
  },
  "scripts": {
    "prepublish": "NODE_ENV=production mrbuilder-webpack --app ../../mr-builder.github.io/beta",
    "start": "mrbuilder-webpack-dev-server",
    "test": "NODE_ENV=production mrbuilder-webpack --app ../../mr-builder.github.io/beta-test"
  },
  "mrbuilder": {
    "plugin": "src/index",
    "plugins": [
      [
        "mrbuilder-plugin-version",
        {
          "module": "~mrbuilder/package.json"
        }
      ],
      "mrbuilder-plugin-filetypes",
      "mrbuilder-plugin-fonts",
      "mrbuilder-plugin-env",
      [
        "mrbuilder-plugin-html",
        false
      ],
      "mrbuilder-plugin-doc-prop-types",
      "mrbuilder",
      [
        "mrbuilder-plugin-webpack",
        {
          "app": "docs",
          "devtool": "cheap-module-eval-source-map"
        }
      ],
      [
        "mrbuilder-plugin-hot",
        false
      ],
      "mrbuilder.wiki",
      [
        "mrbuilder-plugin-clean",
        {
          "allowExternal": true,
          "exclude": ".git"
        }
      ]
    ]
  },
  "license": "0BSD"
}
