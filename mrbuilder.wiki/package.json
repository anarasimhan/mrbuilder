{
  "name": "mrbuilder.wiki",
  "private": true,
  "version": "1.0.2",
  "dependencies": {
    "mrbuilder": "^1.0.0",
    "mrbuilder-plugin-clean": "^1.0.2",
    "mrbuilder-plugin-react-styleguidist": "^1.0.1"
  },
  "scripts": {
    "prepublish": "NODE_ENV=production mrbuilder-webpack --app ../../mr-builder.github.io",
    "start": "mrbuilder-webpack-dev-server"
  },
  "mrbuilder": {
    "plugin": "src/index",
    "plugins": [
      "mrbuilder-plugin-env",
      [
        "mrbuilder-plugin-html",
        false
      ],
      "mrbuilder-plugin-doc-prop-types",
      "mrbuilder",
      [
        "mrbuilder-plugin-webpack",
        {
          "app": "docs"
        }
      ],
      [
        "mrbuilder-plugin-hot",
        false
      ],
      "mrbuilder.wiki",
      [
        "mrbuilder-plugin-clean",
        {
          "allowExternal": true,
          "exclude": ".git"
        }
      ]
    ]
  },
  "license": "0BSD"
}
