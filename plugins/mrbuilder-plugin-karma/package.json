{
  "name": "mrbuilder-plugin-karma",
  "version": "3.1.5",
  "description": "Tools for developing with mrbuilder and karma",
  "scripts": {
    "test": "mrbuilder-mocha"
  },
  "bin": {
    "mrbuilder-karma-cli": "./bin/cli.js"
  },
  "dependencies": {
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mrbuilder-optionsmanager": "^3.1.5",
    "mrbuilder-plugin-enzyme": "^3.1.5",
    "mrbuilder-plugin-mocha": "^3.1.5",
    "mrbuilder-plugin-webpack": "^3.1.5"
  },
  "source": "src",
  "main": "src",
  "mrbuilder": {
    "alias": {
      "mainFields": "Sets main fields for easier debugging"
    },
    "plugins": [
      "mrbuilder-plugin-browserslist",
      "mrbuilder-plugin-webpack",
      "mrbuilder-plugin-enzyme",
      "mrbuilder-plugin-mocha"
    ]
  },
  "license": "0BSD",
  "peerDependencies": {
    "mrbuilder": "3.x"
  },
  "gitHead": "35ac714629506729803ad821911b3f0ed70cb347"
}
