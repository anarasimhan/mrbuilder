{
  "name": "mrbuilder-plugin-webpack-dev-server",
  "version": "0.0.4",
  "description": "webpack dev server for mrbuilder",
  "dependencies": {
    "mrbuilder-plugin-webpack": "^0.0.4",
    "mrbuilder-utils": "^0.0.4",
    "webpack-dev-server": "^2.9.5"
  },
  "source": "src",
  "main": "src",
  "bin": {
    "mrbuilder-webpack-server": "./bin/mrbuilder-webpack-dev-server.js"
  },
  "mrbuilder": {
    "env": {
      "development": {
        "plugins": [
          "mrbuilder-plugin-webpack"
        ]
      }
    }
  }
}
