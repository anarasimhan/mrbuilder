{
	"compileOnSave": false,
	"name": "mrbuilder-plugin-typescript",
	"compilerOptions": {
		"suppressExcessPropertyErrors": true,
		"declaration": true,
		"outDir": "@types",
		"target": "es5",
		"strictNullChecks": false,
		"sourceMap": true,
		"skipLibCheck": true,
		"jsx": "react",
		"lib": [
			"dom",
			"es2015"
		]
	},
	"exclude": [
		"node_modules",
		"**/*.spec.ts",
		"webpack.config.js",
		"./dist"
	]
}
