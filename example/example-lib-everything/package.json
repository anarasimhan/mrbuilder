{
  "name": "example-lib-everything",
  "version": "2.1.0-0",
  "private": true,
  "devDependencies": {
    "mrbuilder": "^2.1.0-0"
  },
  "dependencies": {
    "mrbuilder-plugin-env": "^2.1.0-0",
    "mrbuilder-plugin-lodash": "^2.1.0-0",
    "mrbuilder-plugin-markdown": "^2.1.0-0",
    "mrbuilder-plugin-moment": "^2.1.0-0",
    "mrbuilder-plugin-stylus": "^2.1.0-0",
    "mrbuilder-plugin-worker": "^2.1.0-0",
    "mrbuilder-plugin-yaml": "^2.1.0-0"
  },
  "scripts": {
    "analyze": "mrbuilder",
    "prepublish": "mrbuilder",
    "start": "mrbuilder",
    "test": "mrbuilder",
    "karma": "mrbuilder"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "source": "src",
  "main": "lib",
  "mrbuilder": {
    "presets": [
      "mrbuilder"
    ],
    "plugins": [
      [
        "mrbuilder-plugin-env",
        {
          "environment": [
            "NODE_ENV",
            "USER"
          ]
        }
      ],
      "mrbuilder-plugin-yaml",
      "mrbuilder-plugin-css",
      "mrbuilder-plugin-markdown",
      "mrbuilder-plugin-lodash",
      "mrbuilder-plugin-env",
      "mrbuilder-plugin-moment",
      "mrbuilder-plugin-stylus",
      "mrbuilder-plugin-worker",
      "mrbuilder-plugin-typescript"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "mrbuilder-plugin-html",
            false
          ]
        ]
      },
      "test": {
        "plugins": [
          [
            "mrbuilder-plugin-lodash",
            false
          ]
        ]
      }
    }
  },
  "license": "0BSD"
}
