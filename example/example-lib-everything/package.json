{
  "name": "example-lib-everything",
  "version": "0.0.8",
  "private": true,
  "devDependencies": {
    "mrbuilder": "^0.0.7"
  },
  "dependencies": {
    "babel-eslint": "^8.0.3",
    "mrbuilder-plugin-env": "^0.0.6",
    "mrbuilder-plugin-lodash": "^0.0.6",
    "mrbuilder-plugin-markdown": "^0.0.8",
    "mrbuilder-plugin-moment": "^0.0.6",
    "mrbuilder-plugin-stylus": "^0.0.6",
    "mrbuilder-plugin-uglify": "^0.0.6",
    "mrbuilder-plugin-worker": "^0.0.6",
    "mrbuilder-plugin-eslint": "^0.0.6"
  },
  "scripts": {
    "analyze": "MRBUILDER_PLUGINS=mrbuilder-plugin-analyze mrbuilder-webpack",
    "prepublish": "mrbuilder-webpack --mrbuilder-plugin-env-environment=[NODE_ENV,USER]",
    "start": "mrbuilder-webpack-dev-server --mrbuilder-plugin-env-environment=[NODE_ENV,USER]",
    "test": "mrbuilder-karma",
    "karma": "mrbuilder-karma"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "source": "src",
  "main": "lib",
  "mrbuilder": {
    "presets": [
      "mrbuilder"
    ],
    "plugins": [
      "mrbuilder-plugin-css",
      "mrbuilder-plugin-markdown",
      "mrbuilder-plugin-lodash",
      "mrbuilder-plugin-env",
      "mrbuilder-plugin-moment",
      "mrbuilder-plugin-stylus",
      "mrbuilder-plugin-uglify",
      "mrbuilder-plugin-worker",
      [
        "mrbuilder-plugin-html",
        false
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "mrbuilder-plugin-eslint",
          "mrbuilder-plugin-css",
          "mrbuilder-plugin-markdown",
          "mrbuilder-plugin-env",
          "mrbuilder-plugin-moment",
          "mrbuilder-plugin-stylus",
          "mrbuilder-plugin-worker",
          "mrbuilder-plugin-eslint"
        ]
      },
      "development": {
        "plugins": [
          "mrbuilder-plugin-eslint",
          "mrbuilder-plugin-css",
          "mrbuilder-plugin-markdown",
          "mrbuilder-plugin-lodash",
          "mrbuilder-plugin-env",
          "mrbuilder-plugin-moment",
          "mrbuilder-plugin-stylus",
          "mrbuilder-plugin-worker",
          "mrbuilder-plugin-hot"
        ]
      }
    }
  }
}
