{
  "name": "mrbuilder-preset-app",
  "version": "3.0.3",
  "dependencies": {
    "mrbuilder-plugin-babel": "^3.0.2",
    "mrbuilder-plugin-browserslist": "^3.0.1",
    "mrbuilder-plugin-chunk": "^3.0.1",
    "mrbuilder-plugin-css": "^3.0.2",
    "mrbuilder-plugin-env": "^3.0.1",
    "mrbuilder-plugin-filetypes": "^3.0.1",
    "mrbuilder-plugin-fonts": "^3.0.2",
    "mrbuilder-plugin-hot": "^3.0.1",
    "mrbuilder-plugin-html": "^3.0.2",
    "mrbuilder-plugin-less": "^3.0.2",
    "mrbuilder-plugin-uglify": "^3.0.2",
    "mrbuilder-plugin-webpack": "^3.0.3",
    "mrbuilder-plugin-webpack-dev-server": "^3.0.3"
  },
  "peerDependencies": {
    "mrbuilder": "3.x"
  },
  "mrbuilder": {
    "plugins": [
      "mrbuilder-plugin-babel",
      "mrbuilder-plugin-browserslist",
      "mrbuilder-plugin-filetypes",
      "mrbuilder-plugin-fonts",
      "mrbuilder-plugin-env",
      "mrbuilder-plugin-less"
    ],
    "env": {
      "test": {
        "presets": [
          "mrbuilder-preset-test"
        ]
      },
      "development": {
        "presets": [
          "mrbuilder-preset-dev"
        ]
      },
      "production": {
        "plugins": [
          [
            "mrbuilder-plugin-css",
            {
              "useNameHash": "css/[name].[hash].css"
            }
          ],
          [
            "mrbuilder-plugin-webpack",
            {
              "app": "app",
              "filename": "js/[name].[hash].js",
              "externalizePeers": false,
              "useExternals": false
            }
          ],
          [
            "mrbuilder-plugin-chunk",
            {
              "filename": "js/[name].[hash].js"
            }
          ],
          [
            "mrbuilder-plugin-html",
            {
              "exported": true
            }
          ],
          "mrbuilder-plugin-uglify"
        ]
      }
    }
  },
  "license": "0BSD"
}
