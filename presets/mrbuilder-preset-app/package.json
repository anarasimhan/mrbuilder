{
  "name": "mrbuilder-preset-app",
  "version": "0.9.11",
  "dependencies": {
    "mrbuilder-plugin-chunk": "^0.9.11",
    "mrbuilder-plugin-env": "^0.9.11",
    "mrbuilder-plugin-hot": "^0.9.11",
    "mrbuilder-plugin-html": "^0.9.11",
    "mrbuilder-plugin-uglify": "^0.9.11",
    "mrbuilder-plugin-webpack-dev-server": "^0.9.11"
  },
  "peerDependencies": {
    "mrbuilder": "^0.5.0"
  },
  "mrbuilder": {
    "plugins": [
      "mrbuilder",
      "mrbuilder-plugin-env",
      [
        "mrbuilder-plugin-html",
        {
          "exported": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "mrbuilder-plugin-html",
            false
          ]
        ]
      },
      "development": {
        "plugins": [
          [
            "mrbuilder-plugin-css",
            {
              "useNameHash": "css/[name].css"
            }
          ],
          "mrbuilder-plugin-hot"
        ]
      },
      "production": {
        "plugins": [
          [
            "mrbuilder-plugin-css",
            {
              "useNameHash": "css/[name].[hash].css"
            }
          ],
          [
            "mrbuilder-plugin-webpack",
            {
              "app": "app",
              "filename": "js/[name].[hash].js",
              "externalizePeers": false
            }
          ],
          [
            "mrbuilder-plugin-chunk",
            {
              "filename": "js/[name].[hash].js"
            }
          ],
          "mrbuilder-plugin-uglify"
        ]
      }
    }
  },
  "license": "0BSD"
}
