{
  "name": "mrbuilder-preset-dev",
  "version": "3.1.5",
  "dependencies": {
    "mrbuilder-plugin-css": "^3.1.5",
    "mrbuilder-plugin-eslint": "^3.1.5",
    "mrbuilder-plugin-hot": "^3.1.5",
    "mrbuilder-plugin-react": "^3.1.5",
    "mrbuilder-plugin-webpack-dev-server": "^3.1.5",
    "mrbuilder-preset-app": "^3.1.5"
  },
  "peerDependencies": {
    "mrbuilder": "3.x"
  },
  "mrbuilder": {
    "presets": [
      "mrbuilder-preset-app"
    ],
    "plugins": [
      "mrbuilder-plugin-react",
      "mrbuilder-plugin-eslint",
      [
        "mrbuilder-plugin-html",
        {
          "exported": true
        }
      ],
      "mrbuilder-plugin-hot",
      [
        "mrbuilder-plugin-css",
        {
          "useNameHash": "[name].css",
          "useStyleLoader": true
        }
      ],
      [
        "mrbuilder-plugin-webpack",
        {
          "useExternals": false,
          "externalizePeers": false
        }
      ],
      "mrbuilder-plugin-webpack-dev-server"
    ]
  },
  "license": "0BSD",
  "gitHead": "35ac714629506729803ad821911b3f0ed70cb347"
}
