{
  "name": "mrbuilder-preset-lib",
  "version": "2.1.0-0",
  "dependencies": {
    "mrbuilder-plugin-babel": "^2.1.0-0",
    "mrbuilder-plugin-browserslist": "^2.1.0-0",
    "mrbuilder-plugin-css": "^2.1.0-0",
    "mrbuilder-plugin-env": "^2.1.0-0",
    "mrbuilder-plugin-enzyme": "^2.1.0-0",
    "mrbuilder-plugin-eslint": "^2.1.0-0",
    "mrbuilder-plugin-filetypes": "^2.1.0-0",
    "mrbuilder-plugin-fonts": "^2.1.0-0",
    "mrbuilder-plugin-hot": "^2.1.0-0",
    "mrbuilder-plugin-html": "^2.1.0-0",
    "mrbuilder-plugin-react": "^2.1.0-0",
    "mrbuilder-plugin-webpack": "^2.1.0-0",
    "mrbuilder-plugin-webpack-dev-server": "^2.1.0-0"
  },
  "peerDependencies": {
    "mrbuilder": "2.x"
  },
  "mrbuilder": {
    "plugins": [
      "mrbuilder-plugin-babel",
      "mrbuilder-plugin-react",
      "mrbuilder-plugin-browserslist",
      "mrbuilder-plugin-env",
      "mrbuilder-plugin-fonts",
      "mrbuilder-plugin-filetypes",
      [
        "mrbuilder-plugin-css",
        {
          "useNameHash": "style.css",
          "useStyleLoader": false
        }
      ],
      [
        "mrbuilder-plugin-uglify",
        false
      ],
      [
        "mrbuilder-plugin-chunk",
        false
      ],
      [
        "mrbuilder-plugin-webpack",
        {
          "externalizePeers": true,
          "libraryTarget": "umd",
          "useExternals": true
        }
      ],
      "mrbuilder-plugin-less"
    ],
    "env": {
      "test": {
        "presets": [
          "mrbuilder-preset-test"
        ]
      },
      "development": {
        "presets": [
          "mrbuilder-preset-dev"
        ]
      }
    }
  },
  "license": "0BSD"
}
