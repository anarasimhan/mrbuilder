{
  "name": "mrbuilder-preset-lib",
  "version": "2.0.4",
  "dependencies": {
    "mrbuilder-plugin-babel": "^2.0.4",
    "mrbuilder-plugin-browserslist": "^2.0.4",
    "mrbuilder-plugin-chunk": "^2.0.4",
    "mrbuilder-plugin-css": "^2.0.4",
    "mrbuilder-plugin-env": "^2.0.4",
    "mrbuilder-plugin-enzyme": "^2.0.4",
    "mrbuilder-plugin-eslint": "^2.0.4",
    "mrbuilder-plugin-filetypes": "^2.0.4",
    "mrbuilder-plugin-fonts": "^2.0.4",
    "mrbuilder-plugin-hot": "^2.0.4",
    "mrbuilder-plugin-html": "^2.0.4",
    "mrbuilder-plugin-karma": "^2.0.4",
    "mrbuilder-plugin-less": "^2.0.4",
    "mrbuilder-plugin-mocha": "^2.0.4",
    "mrbuilder-plugin-react": "^2.0.4",
    "mrbuilder-plugin-webpack": "^2.0.4",
    "mrbuilder-plugin-webpack-dev-server": "^2.0.4"
  },
  "peerDependencies": {
    "mrbuilder": "2.x"
  },
  "mrbuilder": {
    "plugins": [
      "mrbuilder-plugin-babel",
      "mrbuilder-plugin-react",
      "mrbuilder-plugin-browserslist",
      "mrbuilder",
      "mrbuilder-plugin-env",
      "mrbuilder-plugin-fonts",
      "mrbuilder-plugin-less",
      "mrbuilder-plugin-filetypes",
      [
        "mrbuilder-plugin-css",
        {
          "useNameHash": "[name].css"
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "mrbuilder-plugin-enzyme",
          "mrbuilder-plugin-mocha",
          "mrbuilder-plugin-karma",
          [
            "mrbuilder-plugin-css",
            {
              "useNameHash": "[name].css",
              "useStyleLoader": true
            }
          ]
        ]
      },
      "development": {
        "plugins": [
          "mrbuilder-plugin-eslint",
          "mrbuilder-plugin-html",
          "mrbuilder-plugin-webpack-dev-server",
          "mrbuilder-plugin-hot",
          [
            "mrbuilder-plugin-css",
            {
              "useNameHash": "[name].css",
              "useStyleLoader": true
            }
          ]
        ]
      },
      "production": {
        "plugins": [
          [
            "mrbuilder-plugin-webpack",
            {
              "externalizePeers": true,
              "libraryTarget": "umd",
              "useExternals": true
            }
          ]
        ]
      }
    }
  },
  "license": "0BSD"
}
