{
  "name": "mrbuilder-preset-test",
  "version": "2.0.4",
  "dependencies": {
    "mrbuilder-plugin-enzyme": "^2.0.4",
    "mrbuilder-plugin-karma": "^2.0.4",
    "mrbuilder-plugin-mocha": "^2.0.4",
    "mrbuilder-plugin-webpack": "^2.0.4",
    "mrbuilder-plugin-css": "^2.0.4"
  },
  "peerDependencies": {
    "mrbuilder": "2.x"
  },
  "mrbuilder": {
    "plugins": [
      [
        "mrbuilder-plugin-webpack",
        {
          "useExternals": false,
          "externalizePeers": false
        }
      ],
      [
        "mrbuilder-plugin-html",
        false
      ],
      [
        "mrbuilder-plugin-css",
        {
          "useNameHash": "[id].[name].css",
          "useStyleLoader": true
        }
      ],
      [
        "mrbuilder-plugin-html",
        false
      ],
      [
        "mrbuilder-plugin-hot",
        false
      ],
      "mrbuilder-plugin-enzyme",
      "mrbuilder-plugin-mocha",
      "mrbuilder-plugin-webpack",
      "mrbuilder-plugin-karma"
    ]
  },
  "license": "0BSD"
}
